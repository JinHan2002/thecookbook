 });\n      });\n  };\n\n  onChange = (event) => {\n    this.setState({ [event.target.name]: event.target.value });\n  };\n\n  render() {\n    const { email, password, error } = this.state;\n    return (\n      <div id=\"signin\" justify=\"center\">\n        <Header> Sign In </Header>\n        <Grid container spacing={1} alignItems=\"flex-end\" justify=\"center\">\n          <IconAlign>\n            <Grid item>\n              <EmailIcon fontSize=\"large\" />\n            </Grid>\n          </IconAlign>\n          <Grid item>\n            <TextField\n              id=\"email\"\n              name=\"email\"\n              label=\"Email\"\n              onChange={this.onChange}\n              value={email}\n              fullWidth\n              variant=\"outlined\"\n              size=\"small\"\n              margin=\"normal\"\n              InputProps={{ style: { fontSize: 14 } }}\n              InputLabelProps={{ style: { fontSize: 12 } }}\n            />\n          </Grid>\n        </Grid>\n        <Grid container spacing={1} alignItems=\"flex-end\" justify=\"center\">\n          <IconAlign>\n            <Grid item>\n              <LockIcon fontSize=\"large\" />\n            </Grid>\n          </IconAlign>\n          <Grid item>\n            <TextField\n              id=\"password\"\n              name=\"password\"\n              label=\"Password\"\n              type=\"password\"\n              onChange={this.onChange}\n              value={password}\n              fullWidth\n              variant=\"outlined\"\n              size=\"small\"\n              margin=\"normal\"\n              InputProps={{ style: { fontSize: 14 } }}\n              InputLabelProps={{ style: { fontSize: 12 } }}\n            />\n          </Grid>\n        </Grid>\n        <Grid container spacing={1} alignItems=\"flex-end\" justify=\"center\">\n          <BtnNoneOutLine\n            onClick={this.onSubmit}\n            disabled={!email || !password}\n            variant=\"contained\"\n     